[run]
source = mem8
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */migrations/*
    .venv/*
data_file = reports/.coverage

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = reports/htmlcov

[xml]
output = reports/coverage.xml