      <CodeComponent name="updateEarnings" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="#initialize the overall earnings&amp;#10;bankedEarnings=0.0&amp;#10;&amp;#10;# some text to show the earnings on screen (starts empty)&amp;#10;balloonEarnings = ''&amp;#10;&amp;#10;# and some text to show the banked earnings (starts blank)&amp;#10;bankedText = ''&amp;#10;&amp;#10;#initialize the earnings on the last balloon &amp;#10;lastBalloonEarnings=0.0&amp;#10;thisBalloonEarnings=0.0" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="bankedEarnings = 0.0;&amp;#10;balloonEarnings = &quot;&quot;;&amp;#10;bankedText = &quot;&quot;;&amp;#10;lastBalloonEarnings = 0.0;&amp;#10;thisBalloonEarnings = 0.0;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="thisBalloonEarnings = ((pump.thisN + 1) * 0.05);&amp;#10;balloonEarnings = (&quot;This balloon value:\n\u00a3&quot; + util.round(thisBalloonEarnings, 2).toString());&amp;#10;bankedText = (&quot;You have banked:\n\u00a3&quot; + util.round(bankedEarnings, 2).toString());&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# you earn £0.05 per pump (until it pops) &amp;#10;thisBalloonEarnings=(pump.thisN+1)*0.05 # &quot;pump&quot; corresponds to the name of this loop&amp;#10;&amp;#10;#update the text to show onscreen the actual earnings in this trial and the banked earnings&amp;#10;balloonEarnings = &quot;This balloon value:\n£&quot; + str(round(thisBalloonEarnings, 2))&amp;#10;bankedText = &quot;You have banked:\n£&quot; + str(round(bankedEarnings, 2))" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type"/>
        <Param val="" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="var _pj;&amp;#10;function _pj_snippets(container) {&amp;#10;    function in_es6(left, right) {&amp;#10;        if (((right instanceof Array) || ((typeof right) === &quot;string&quot;))) {&amp;#10;            return (right.indexOf(left) &gt; (- 1));&amp;#10;        } else {&amp;#10;            if (((right instanceof Map) || (right instanceof Set) || (right instanceof WeakMap) || (right instanceof WeakSet))) {&amp;#10;                return right.has(left);&amp;#10;            } else {&amp;#10;                return (left in right);&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;    container[&quot;in_es6&quot;] = in_es6;&amp;#10;    return container;&amp;#10;}&amp;#10;_pj = {};&amp;#10;_pj_snippets(_pj);&amp;#10;if (((pump.thisN + 1) === maxPumps)) {&amp;#10;    popped = true;&amp;#10;} else {&amp;#10;    popped = false;&amp;#10;}&amp;#10;if (popped) {&amp;#10;    thisBalloonEarnings = 0.0;&amp;#10;    lastBalloonEarnings = 0.0;&amp;#10;} else {&amp;#10;    lastBalloonEarnings = thisBalloonEarnings;&amp;#10;}&amp;#10;if (_pj.in_es6(&quot;return&quot;, bankButton.keys)) {&amp;#10;    pump.finished = true;&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="#if the pump number exceeds the max pumps... then set popped to True&amp;#10;if pump.thisN+1 == maxPumps:&amp;#10;    popped = True&amp;#10;else:&amp;#10;    popped = False&amp;#10;&amp;#10;# if balloon popped reset the earnings on this trial to 0&amp;#10;if popped:&amp;#10;  thisBalloonEarnings=0.0&amp;#10;  lastBalloonEarnings=0.0&amp;#10;else:&amp;#10;    lastBalloonEarnings=thisBalloonEarnings&amp;#10;&amp;#10;# if the user presses return end this loop &amp;#10;if 'return' in bankButton.keys:&amp;#10;    pump.finished = True&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>
        <Param val="False" valType="bool" updates="None" name="disabled"/>
        <Param val="updateEarnings" valType="code" updates="None" name="name"/>
      </CodeComponent>
