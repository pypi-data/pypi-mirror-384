        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="// make sure correct answers were read as floats&amp;#10;targetPos = {&amp;#10;    white: whiteSquarePos.map(parseFloat),&amp;#10;    pink: pinkSquarePos.map(parseFloat)&amp;#10;}&amp;#10;// do same as in Python, but with targetPos.___ rather than ___SquarePos&amp;#10;whiteSquareCorr = ((((targetPos.white[0] - 0.1) &lt; whiteSquare.pos[0]) &amp;&amp; (whiteSquare.pos[0] &lt; (targetPos.white[0] + 0.1))) &amp;&amp; (((targetPos.white[1] - 0.1) &lt; whiteSquare.pos[1]) &amp;&amp; (whiteSquare.pos[1] &lt; (targetPos.white[1] + 0.1))));&amp;#10;pinkSquareCorr = ((((targetPos.pink[0] - 0.1) &lt; pinkSquare.pos[0]) &amp;&amp; (pinkSquare.pos[0] &lt; (targetPos.pink[0] + 0.1))) &amp;&amp; (((targetPos.pink[1] - 0.1) &lt; pinkSquare.pos[1]) &amp;&amp; (pinkSquare.pos[1] &lt; (targetPos.pink[1] + 0.1))));&amp;#10;corr = (whiteSquareCorr &amp;&amp; pinkSquareCorr);&amp;#10;psychoJS.experiment.addData(&quot;whiteSquare.pos&quot;, whiteSquare.pos);&amp;#10;psychoJS.experiment.addData(&quot;pinkSquare.pos&quot;, pinkSquare.pos);&amp;#10;psychoJS.experiment.addData(&quot;whiteSquare.corr&quot;, whiteSquareCorr);&amp;#10;psychoJS.experiment.addData(&quot;pinkSquare.corr&quot;, pinkSquareCorr);&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="# each square is correct if it's within 0.1 units of target position&amp;#10;whiteSquareCorr = (&amp;#10;    whiteSquarePos[0] - 0.1 &lt; whiteSquare.pos[0] &lt; whiteSquarePos[0] + 0.1&amp;#10;    and whiteSquarePos[1] - 0.1 &lt; whiteSquare.pos[1] &lt; whiteSquarePos[1] + 0.1&amp;#10;)&amp;#10;pinkSquareCorr = (&amp;#10;    pinkSquarePos[0] - 0.1 &lt; pinkSquare.pos[0] &lt; pinkSquarePos[0] + 0.1&amp;#10;    and pinkSquarePos[1] - 0.1 &lt; pinkSquare.pos[1] &lt; pinkSquarePos[1] + 0.1&amp;#10;)&amp;#10;# trial is correct if both squares are correct&amp;#10;corr = whiteSquareCorr and pinkSquareCorr&amp;#10;# save positions and correct&amp;#10;thisExp.addData(&quot;whiteSquare.pos&quot;, whiteSquare.pos)&amp;#10;thisExp.addData(&quot;pinkSquare.pos&quot;, pinkSquare.pos)&amp;#10;thisExp.addData(&quot;whiteSquare.corr&quot;, whiteSquareCorr)&amp;#10;thisExp.addData(&quot;pinkSquare.corr&quot;, pinkSquareCorr)" valType="extendedCode" updates="constant" name="End Routine"/>
