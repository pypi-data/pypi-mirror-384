{
	"line_subst" : [
		{"from" : "YYYYMMDD", "to" : "yyyyMMdd", "case_sensitive_match": "1"},
		{"from" : "\bCOALESCE\b", "to" : "coalesce", "case_sensitive_match": "1"},
		{"from" : "#(\w+?)\.\$(\w+?)#", "to" : "{$1}"},
		{"from" : "#(\w+?)#", "to" : "{$1}"},
		{"from" : "\s+IS\s+NULL", "to" : ".isNull()"},
		{"from" : "(\bTO_CHAR\s*\([0-9]+\s*)\-", "to" : "$1,"},
		{"from" : "\s+IS\s+not\s+NULL", "to" : ".isNotNull()"},
		{"from" : "99999+", "to" : "9999"},
		{"from" : "20501230", "to" : "2050-12-30"}
	],

	"function_subst" : [
		{"from" : "IF", "to" : "IIF"}
		
		,{"from" : "TO_NUMBER", "output_template" : "cast($1 as int)"}
		,{"from" : "to_char", "arg_pattern" : {"1" : "[0-9]+"}, "output_template" : "date_sub(to_date($1-12-31,'yyyy-MM-dd'),$2)", "num_args" : 2}
		,{"from" : "to_char", "to" : "date_format"}
		,{"from" : "EPM_TO_DATE", "to" : "date_format"}
		,{"from" : "decode", "output_template" : "CASE WHEN $1 = $2 THEN to_date($3,'yyyy-MM-dd') ELSE to_date($4,'yyyy-MM-dd') END ", "num_args" : 4}
		
		,{"from" : "Not", "to" : "~col"}
	]
}