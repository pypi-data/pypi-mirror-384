<mvc:View
	controllerName="com.optrabot.ui.controller.TradeTemplates"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<f:FlexibleColumnLayout id="flexibleColumnLayout" backgroundDesign="Solid" stateChange=".onStateChanged">
		<f:beginColumnPages>
			<mvc:XMLView id="beginView" viewName="com.optrabot.ui.view.TradeTemplateList"/>
		</f:beginColumnPages>
		<f:midColumnPages>
			<mvc:XMLView id="detailView" viewName="com.optrabot.ui.view.TradeTemplateDetail"/>
		</f:midColumnPages>
	</f:FlexibleColumnLayout>
</mvc:View>