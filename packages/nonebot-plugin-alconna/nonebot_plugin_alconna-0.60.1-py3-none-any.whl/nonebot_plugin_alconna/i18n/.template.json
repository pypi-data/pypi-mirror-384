
{
  "$schema": "./.template.schema.json",
  "scopes": [
    {
      "scope": "nbp-alc",
      "types": [
        {
          "subtype": "completion",
          "types": [
            "tab",
            "enter",
            "exit",
            "other",
            "timeout",
            "exited"
          ]
        },
        {
          "subtype": "log",
          "types": [
            "load_global_extensions",
            {
              "subtype": "got_path",
              "types": [
                "ms",
                "validate"
              ]
            },
            {
              "subtype": "discord",
              "types": [
                "ambiguous_command",
                "ambiguous_subcommand"
              ]
            },
            "parse"
          ]
        },
        {
          "subtype": "error",
          "types": [
            "discord_prefix",
            "existed_command",
            {
              "subtype": "extension",
              "types": [
                "forbid_exclude",
                "path_load",
                "path_invalid"
              ]
            },
            "matcher_got_path"
          ]
        },
        {
          "subtype": "test",
          "types": [
            "command_unusable",
            "parse_failed",
            "check_failed",
            "passed"
          ]
        }
      ]
    },
    {
      "scope": "nbp-alc/builtin",
        "types": [
          {
            "subtype": "lang",
            "types": [
              {
                "subtype": "help",
                "types": [
                  "list",
                  "switch",
                  "main"
                ]
              },
              "list",
              "switch",
              "locale_missing",
              "locale_timeout",
              "config_name_error"
            ]
          },
          {
            "subtype": "help",
            "types": [
              "plugin_name_unknown",
              "plugin_name",
              "plugin_id",
              "plugin_path",
              "plugin_module",
              "plugin_version"
            ]
          }
        ]
    }
  ]
}
