# Finnhub API Configuration (Optional - for alternative data & sentiment)
FINNHUB_API_KEY=your_finnhub_api_key_here

# Tiingo API Configuration (Optional - for historical fundamentals)
TIINGO_API_KEY=your_tiingo_api_key_here

