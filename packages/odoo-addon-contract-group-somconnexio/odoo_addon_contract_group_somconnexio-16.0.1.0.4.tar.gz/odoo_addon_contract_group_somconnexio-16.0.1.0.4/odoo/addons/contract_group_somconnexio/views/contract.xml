<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="contract_contract_contract_group" model="ir.ui.view">
        <field name="name">contract.contract.contract.group.view.form.somconnexio</field>
        <field name="model">contract.contract</field>
        <field name="inherit_id" ref="contract_group.contract_contract_contract_group"/>
        <field name="arch" type="xml">
            <field name="contract_group_id" position="attributes">
                <attribute name="readonly">1</attribute>
            </field>
        </field>
    </record>
    <record id="contract_contract_contract_group_tree_view" model="ir.ui.view">
        <field name="name">contract.contract.contract.group.view.tree.somconnexio</field>
        <field name="model">contract.contract</field>
        <field name="inherit_id" ref="somconnexio.contract_contract_customer_tree_view"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="contract_group_id"/>
            </field>
        </field>
    </record>
</odoo>
