base_store:
      #type: SQL
      type: Memory
      dialect: mysql+pymysql  # the dialect used to interact with the db
      #host: 127.0.0.1
      host: your-database-host.com
      port: 3306
      db: your_database_name  # path to your db
      username: your_username # username used for authentication
      password: your_password # password used for authentication

channel:
      type: redis
      host: localhost # Redis server host
      port: 6379 # Redis server port
      db: 0 # Redis database number
      password: your_redis_password # Redis password, if any

llm:
      type: openai
      model: deepseek-chat
      #model: qwen-max
      #model: glm-4-0520
      key: your-deepseek-api-key #deepseek
      apibase: https://api.deepseek.com
      #key: your-qwen-api-key  #aliyun dashscope
      #apibase: https://dashscope.aliyuncs.com/compatible-mode/v1
      #key: your-chatglm-api-key #chatglm
      #apibase: https://open.bigmodel.cn/api/paas/v4/
