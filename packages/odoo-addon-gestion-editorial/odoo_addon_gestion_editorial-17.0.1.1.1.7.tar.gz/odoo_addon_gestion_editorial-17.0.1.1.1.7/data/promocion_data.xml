<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <record id="stock_location_promocion" model="stock.location">
            <field name="name">Promoción</field>
            <field name="location_id" ref="stock.stock_location_locations_partner"/>
            <field name="usage">inventory</field>
            <field name="company_id"></field>
        </record>
        <record id="stock_picking_type_promocion" model="stock.picking.type">
            <field name="name">Libros a promoción</field>
            <field name="sequence_code">LIB_PROMO</field>
            <field name="code">outgoing</field>
            <field name="show_operations">False</field>
            <field name="use_existing_lots">False</field>
            <field name="warehouse_id">1</field>
            <field name="default_location_src_id" ref="stock.stock_location_stock"/>
            <field name="default_location_dest_id" ref="stock_location_promocion"/>
        </record>
    </data>
</odoo>
