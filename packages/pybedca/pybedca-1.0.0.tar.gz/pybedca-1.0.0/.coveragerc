[run]
source = src/pybedca
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */.venv/*
    */build/*
    */dist/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

show_missing = True
skip_covered = False
precision = 2

[html]
directory = htmlcov
title = PyBEDCA Coverage Report

[xml]
output = coverage.xml