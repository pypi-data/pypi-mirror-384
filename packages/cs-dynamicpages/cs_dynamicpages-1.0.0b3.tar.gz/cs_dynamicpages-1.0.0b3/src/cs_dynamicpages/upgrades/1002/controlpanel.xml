<?xml version="1.0" encoding="utf-8"?>
<object xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        name="portal_controlpanel"
        i18n:domain="cs_dynamicpages"
>

  <!-- -*- extra stuff goes here -*- -->

  <configlet action_id="dynamica_pages_control_panel-controlpanel"
             remove="true"
  >
  </configlet>


  <configlet action_id="dynamic_pages_control_panel-controlpanel"
             appId="dynamic_pages_control_panel-controlpanel"
             category="Products"
             condition_expr=""
             icon_expr=""
             title="Dynamic Pages Control Panel"
             url_expr="string:${portal_url}/@@dynamic_pages_control_panel-controlpanel"
             visible="True"
             i18n:attributes="title"
  >
    <permission>Manage Portal</permission>
  </configlet>


</object>
