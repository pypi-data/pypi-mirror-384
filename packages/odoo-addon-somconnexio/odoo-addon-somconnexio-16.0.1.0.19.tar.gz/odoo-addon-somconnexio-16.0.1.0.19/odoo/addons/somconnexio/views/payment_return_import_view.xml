<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="payment_return_import_form_view" model="ir.ui.view">
    <field name="name">Import Payment Returns</field>
    <field name="model">payment.return.import</field>
    <field name="priority">1</field>
    <field name="inherit_id" ref="account_payment_return_import.account_payment_return_import_view"/>
    <field name="arch" type="xml">
      <field name="journal_id" position="attributes">
        <attribute name="domain">['|',('type', '=', 'bank'),('type', '=', 'general')]</attribute>
      </field>
    </field>
  </record>
</odoo>
