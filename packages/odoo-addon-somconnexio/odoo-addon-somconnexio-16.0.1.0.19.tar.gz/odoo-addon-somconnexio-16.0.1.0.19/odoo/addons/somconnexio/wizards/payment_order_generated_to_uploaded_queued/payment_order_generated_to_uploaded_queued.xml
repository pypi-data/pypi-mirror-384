<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="payment_order_generated_to_uploaded_action" model="ir.actions.act_window">
    <field name="name">Set as uploaded queued</field>
    <field name="res_model">payment.order.generated.uploaded.queued</field>
    <field name="binding_model_id" ref="account_payment_order.model_account_payment_order"/>
    <field name="view_mode">form</field>
    <field name="target">new</field>
    <field name="context">{}</field>
    <field name="binding_type">action</field>
  </record>
  <record id="payment_order_generated_to_uploaded_form" model="ir.ui.view">
    <field name="name">payment_order_generated_to_uploaded.form</field>
    <field name="model">payment.order.generated.uploaded.queued</field>
    <field name="arch" type="xml">
      <form string="Create Payment Lines">
        <p>This wizard will set as uploaded already generated payment orders:</p>
        <footer>
          <button type="object" name="run" string="Create" class="oe_highlight"/>
          <button special="cancel" string="Cancel" class="oe_link"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
