<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <function model="somconnexio.module" name="clean_demo_data"/>
    <function model="somconnexio.module" name="install_languages"/>
    <function model="somconnexio.module" name="import_bank_data"/>
    <function model="somconnexio.module" name="load_fiscal_location"/>
    <record id="base.main_company" model="res.company">
      <field name="name">Som Connexió SCCL</field>
      <field name="street">Carrer de les Moreres 119</field>
      <field name="city">El Prat del Llobregat</field>
      <field name="state_id" ref="base.state_es_b"/>
      <field name="zip">08820</field>
      <field name="vat">F66380676</field>
      <field name="currency_id" search="[('name', '=', 'EUR')]"/>
      <field name="logo" type="base64" file="somconnexio/static/img/sc-image.png"/>
      <field name="country_id" ref="base.es"/>
      <field name="website">https://somosconexion.coop</field>
      <field name="phone">931 311 728</field>
      <field name="email">info@somosconexion.coop</field>
      <!-- UNCOMMENT <field name="chart_template_id" ref="somconnexio.account_chart_template_sc"/> -->
      <!-- UNCOMMENT <field name="property_cooperator_account" ref="somconnexio.1_account_sc_44000001"/> -->
      <field name="social_twitter">https://twitter.com/SomConnexio</field>
      <field name="social_facebook">https://es-es.facebook.com/somconnexio</field>
      <field name="social_linkedin">https://www.linkedin.com/company/somconnexi%C3%B3/</field>
      <field name="social_instagram">https://www.instagram.com/somconnexio/</field>
      <field name="social_youtube">https://www.youtube.com/channel/UCx41B2a9MH1w7I0kbnXEu4A/</field>
      <field name="initiating_party_identifier">F66380676000</field>
      <field name="sepa_creditor_identifier">ES26000F66380676</field>
      <field name="account_sale_tax_id" ref="l10n_es.1_account_tax_template_s_iva21s"/>
      <field name="account_purchase_tax_id" ref="l10n_es.1_account_tax_template_p_iva21_sc"/>
    </record>
    <!-- Som Connexió partner bank-->
    <record id="caixa_guissona" model="res.partner.bank">
      <field name="acc_number">ES90 3140 0001 9000 1206 8500</field>
      <field name="partner_id" model="res.company" eval="obj().env.ref('base.main_company').partner_id.id"/>
      <field name="bank_id" ref="l10n_es_partner.res_bank_es_3140"/>
    </record>
  </data>
</odoo>
