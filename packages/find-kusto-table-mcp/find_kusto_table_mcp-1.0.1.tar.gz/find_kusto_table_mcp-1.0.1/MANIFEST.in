# MANIFEST.in - Include additional files in the distribution

include LICENSE
include README.md
include requirements.txt
include fastmcp.json

recursive-include src *.py *.json
recursive-include prompts *.md
recursive-include docs *.md

# Exclude test and development files
recursive-exclude tests *
recursive-exclude scripts *
recursive-exclude __pycache__ *
recursive-exclude *.pyc
recursive-exclude .git *
recursive-exclude cache *
recursive-exclude logs *
recursive-exclude exports *
recursive-exclude todo *
