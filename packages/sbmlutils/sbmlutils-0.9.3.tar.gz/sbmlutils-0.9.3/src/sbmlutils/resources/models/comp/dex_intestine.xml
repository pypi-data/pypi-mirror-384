    <listOfReactions>
      <reaction metaid="meta_DEXEX" sboTerm="SBO:0000655" id="DEXEX" name="DEX export" reversible="false" fast="false" compartment="Vmem">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml" style="color: #333;font-family: Helvetica, arial, sans-serif;background-color: white;font-size: 14px;line-height: 1.6;padding-top: 10px;padding: 30px;padding-bottom: 10px;">
            <p style="margin-top: 0 !important;margin-bottom: 0 !important;margin: 15px 0;">Vmax of export is defined relative to the <code style="border: 1px solid #eaeaea;padding: 0 5px;border-radius: 3px;white-space: nowrap;margin: 0 2px;background-color: #f8f8f8;">CYP3A4_Vmax</code> to
ensure that the transport is fast relative to the absorption.
This works because the <code style="white-space: nowrap;padding: 0 5px;border: 1px solid #eaeaea;border-radius: 3px;background-color: #f8f8f8;margin: 0 2px;">CYP3A4_Vmax</code> is on the same scale as the maximal absorption rate.</p>
          </body>
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_DEXEX">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000655"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
        <listOfReactants>
          <speciesReference species="dex" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="dex_ext" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> DEXCYP3A4_Vmax </ci>
                <ci> DEXEX_f </ci>
                <ci> Vin </ci>
                <ci> dex </ci>
              </apply>
              <apply>
                <plus/>
                <ci> DEXEX_Km_dex </ci>
                <ci> dex </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction metaid="meta_DEXCYP3A4" sboTerm="SBO:0000176" id="DEXCYP3A4" name="DEX CYP3A4" reversible="false" fast="false" compartment="Vin">
        <notes>
