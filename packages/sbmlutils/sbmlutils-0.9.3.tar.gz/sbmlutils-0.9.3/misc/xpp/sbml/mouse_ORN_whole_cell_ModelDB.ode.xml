              </piece>
              <piece>
                <cn> 0.5 </cn>
                <apply>
                  <eq/>
                  <ci> x </ci>
                  <cn type="integer"> 0 </cn>
                </apply>
              </piece>
              <piece>
                <cn type="integer"> 1 </cn>
                <apply>
                  <gt/>
                  <ci> x </ci>
                  <cn type="integer"> 0 </cn>
                </apply>
              </piece>
              <otherwise>
                <cn type="integer"> 0 </cn>
