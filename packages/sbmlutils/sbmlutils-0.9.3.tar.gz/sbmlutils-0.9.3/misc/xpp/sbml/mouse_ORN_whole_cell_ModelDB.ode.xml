        </math>
      </functionDefinition>
      <functionDefinition id="il">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci> vcilia </ci>
            </bvar>
            <bvar>
              <ci> gl </ci>
            </bvar>
            <bvar>
              <ci> vl </ci>
            </bvar>
            <apply>
              <times/>
              <ci> gl </ci>
              <apply>
                <minus/>
                <ci> vl </ci>
                <ci> vcilia </ci>
              </apply>
            </apply>
