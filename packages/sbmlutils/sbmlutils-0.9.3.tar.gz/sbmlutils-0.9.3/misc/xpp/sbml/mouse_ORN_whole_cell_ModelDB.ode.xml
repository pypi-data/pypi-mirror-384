              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="voltage">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci> x </ci>
            </bvar>
            <bvar>
              <ci> y </ci>
            </bvar>
            <bvar>
              <ci> z </ci>
            </bvar>
            <bvar>
              <ci> ge2 </ci>
            </bvar>
            <bvar>
              <ci> nakxsoma </ci>
            </bvar>
            <bvar>
              <ci> tau_soma </ci>
            </bvar>
            <apply>
              <times/>
              <ci> tau_soma </ci>
              <apply>
                <plus/>
                <apply>
                  <minus/>
                  <apply>
                    <plus/>
                    <apply>
                      <minus/>
                      <apply>
                        <times/>
                        <cn type="integer"> 3 </cn>
                        <ci> x </ci>
                      </apply>
                      <apply>
                        <power/>
                        <ci> x </ci>
                        <cn type="integer"> 3 </cn>
                      </apply>
                    </apply>
                    <cn type="integer"> 2 </cn>
                  </apply>
                  <ci> nakxsoma </ci>
                </apply>
                <apply>
                  <ci> input </ci>
                  <ci> y </ci>
                  <ci> z </ci>
                  <ci> ge2 </ci>
                </apply>
              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="inhcng">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci> cacam </ci>
            </bvar>
            <bvar>
              <ci> inhmax </ci>
            </bvar>
            <bvar>
              <ci> kinhcng </ci>
            </bvar>
            <bvar>
              <ci> ninhcng </ci>
            </bvar>
            <apply>
              <plus/>
              <cn type="integer"> 1 </cn>
              <apply>
                <times/>
                <apply>
                  <minus/>
                  <ci> inhmax </ci>
                  <cn type="integer"> 1 </cn>
                </apply>
                <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> cacam </ci>
                    <ci> ninhcng </ci>
                  </apply>
                  <apply>
                    <plus/>
                    <apply>
                      <power/>
                      <ci> cacam </ci>
                      <ci> ninhcng </ci>
                    </apply>
                    <apply>
                      <power/>
                      <ci> kinhcng </ci>
                      <ci> ninhcng </ci>
                    </apply>
                  </apply>
                </apply>
              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="icng">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci> camp </ci>
            </bvar>
            <bvar>
              <ci> vcilia </ci>
            </bvar>
            <bvar>
              <ci> cacam </ci>
            </bvar>
            <bvar>
              <ci> cnmax </ci>
            </bvar>
            <bvar>
              <ci> hmc1 </ci>
            </bvar>
            <bvar>
              <ci> inhmax </ci>
            </bvar>
            <bvar>
              <ci> kinhcng </ci>
            </bvar>
            <bvar>
              <ci> n1 </ci>
            </bvar>
            <bvar>
              <ci> ninhcng </ci>
            </bvar>
            <bvar>
              <ci> vcng </ci>
            </bvar>
            <apply>
              <times/>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <ci> cnmax </ci>
                  <apply>
                    <power/>
                    <ci> camp </ci>
                    <ci> n1 </ci>
                  </apply>
                </apply>
                <apply>
                  <plus/>
                  <apply>
                    <power/>
                    <ci> camp </ci>
                    <ci> n1 </ci>
                  </apply>
                  <apply>
                    <power/>
                    <apply>
                      <times/>
                      <apply>
                        <ci> inhcng </ci>
                        <ci> cacam </ci>
                        <ci> ninhcng </ci>
                        <ci> inhmax </ci>
                        <ci> kinhcng </ci>
                      </apply>
                      <ci> hmc1 </ci>
                    </apply>
                    <ci> n1 </ci>
                  </apply>
                </apply>
              </apply>
              <apply>
                <minus/>
                <ci> vcng </ci>
                <ci> vcilia </ci>
              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="icacl">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci> ca </ci>
            </bvar>
            <bvar>
              <ci> vcilia </ci>
            </bvar>
            <bvar>
              <ci> clmax </ci>
            </bvar>
            <bvar>
              <ci> hmc2 </ci>
            </bvar>
            <bvar>
              <ci> n2 </ci>
            </bvar>
            <bvar>
              <ci> vcl </ci>
            </bvar>
            <apply>
              <times/>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <ci> clmax </ci>
                  <apply>
                    <power/>
                    <ci> ca </ci>
                    <ci> n2 </ci>
                  </apply>
                </apply>
                <apply>
                  <plus/>
                  <apply>
                    <power/>
                    <ci> ca </ci>
                    <ci> n2 </ci>
                  </apply>
                  <apply>
                    <power/>
                    <ci> hmc2 </ci>
                    <ci> n2 </ci>
                  </apply>
                </apply>
              </apply>
              <apply>
