          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="mod" name="modulo">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci> x </ci>
            </bvar>
            <bvar>
              <ci> y </ci>
            </bvar>
            <piecewise>
              <piece>
                <apply>
                  <minus/>
                  <ci> x </ci>
                  <apply>
                    <times/>
                    <ci> y </ci>
                    <apply>
                      <ceiling/>
                      <apply>
                        <divide/>
                        <ci> x </ci>
                        <ci> y </ci>
                      </apply>
                    </apply>
                  </apply>
                </apply>
                <apply>
                  <xor/>
                  <apply>
                    <lt/>
                    <ci> x </ci>
                    <cn type="integer"> 0 </cn>
                  </apply>
                  <apply>
                    <lt/>
                    <ci> y </ci>
                    <cn type="integer"> 0 </cn>
                  </apply>
                </apply>
              </piece>
              <otherwise>
                <apply>
                  <minus/>
                  <ci> x </ci>
                  <apply>
                    <times/>
                    <ci> y </ci>
                    <apply>
                      <floor/>
                      <apply>
                        <divide/>
                        <ci> x </ci>
                        <ci> y </ci>
                      </apply>
                    </apply>
                  </apply>
                </apply>
              </otherwise>
            </piecewise>
          </lambda>
        </math>
      </functionDefinition>
