#########################################################################################
#
#  Test run input file control.in 
#
#########################################################################################

#  Physical model
  qpe_calc gw
  xc               pbe0
  RI_method v
  relativistic     atomic_zora scalar

  anacon_type 1
  frequency_points 200
  n_anacon_par 16

# DFT - SCF convergence settings
  occupation_type    gaussian 0.0001
  mixer              pulay
    n_max_pulay        10
    charge_mix_param   0.2
  sc_accuracy_rho    1E-6
  sc_accuracy_eev    1E-4
  sc_accuracy_etot   1E-6
  sc_iter_limit      100
  prodbas_threshold  1.e-4

  override_illconditioning .true.
  
