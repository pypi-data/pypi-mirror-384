/begin MEASUREMENT
    ecuCounter
    "16 bit counter incrementing exactly every 100us in XCP slave time"
    UWORD ecuCounter_
    COMPU_METHOD
    0
    0
    0
    65535
    ECU_ADDRESS 0x2E348
    PHYS_UNIT ""
    /begin IF_DATA XCP
        /begin DAQ_EVENT
            FIXED_EVENT_LIST EVENT 0x0
        /end DAQ_EVENT
    /end IF_DATA
/end MEASUREMENT
