{
  "deltaVersion": "1.1",
  "name": "testDeltaTwin",
  "description": "Any description",
  "license": {
    "name": "LGPLv3",
    "url": "https://www.gnu.org/licenses/gpl-3.0.txt",
    "description": "license description",
    "copyrights": [
      {"company": "Gael Systems","years": [2023]}
    ]
  },
  "resources": {
    "aData": {
      "type": "Data",
      "description": "resource data description",
      "value": "https://domain.net/path/data"
    },
    "aValue": {
      "type": "string",
      "value": "Value"
    }
  },
  "inputs": {
    "in_data": {
      "type": "Data"
    },
    "in_bool": {
      "type": "boolean",
      "value": false
    }
  },
  "outputs": {
    "out_data": {
      "type": "Data"
    }
  },
  "models": {
    "json-formatter": {
      "path": "models/jsonFormatter",
      "type": "python",
      "parameters": {
        "image": "python:3.10",
        "cmd": "python -m json.tool"
      },
      "inputs": {
        "sortedKeys": {
          "type": "boolean",
          "value": false,
          "prefix": "--sorted-keys"
        },
        "json": {
          "type": "Data"
        }
      },
      "outputs": {
        "out": {
          "type": "stdout"
        }
      }
    },
    "band-selector": {
      "path": "models/band-selector",
      "type": "python",
      "parameters": {
        "image": "python:3.8",
        "requirementFiles": ["requirements.txt"],
        "pyFile": "band_extractor.py",
        "cmd": "python $(parameters.pyFile)"
      },
      "inputs": {
        "product": {
          "type": "Data"
        },
        "bandName": {
          "type": "string"
        }
      },
      "outputs": {
        "selectedBand": {
          "type": "Data",
          "glob": "band/*"
        }
      }
    }
  },
  "dependencies": [
    "test-deltatwin1==1.0.0",
    "test-deltatwin2==1.0.1"
  ]
}