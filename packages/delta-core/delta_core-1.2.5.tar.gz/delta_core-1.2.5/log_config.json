{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "console": {"format": "%(asctime)s %(levelname)s %(name)s %(message)s"}
    },
    "handlers": {
        "stdout": {
            "class": "logging.StreamHandler",
            "formatter": "console",
            "stream": "ext://sys.stdout"
        }
    },
    "root": {"level": "INFO", "handlers": ["stdout"]}
}