# Stytch B2B Configuration
STYTCH_PROJECT_ID=your_stytch_project_id_here
STYTCH_SECRET=your_stytch_secret_key_here
STYTCH_ENVIRONMENT=test

# Redis Configuration
STYTCH_REDIS_URL=redis://localhost:6379
STYTCH_REDIS_PASSWORD=
STYTCH_REDIS_DB=0
STYTCH_REDIS_SSL=false

# Cache Configuration
STYTCH_CACHE_TTL=300
STYTCH_CACHE_PREFIX=ayz_auth

# Logging Configuration
STYTCH_LOG_LEVEL=INFO
STYTCH_LOG_SENSITIVE_DATA=false

# Request Configuration
STYTCH_REQUEST_TIMEOUT=10
STYTCH_MAX_RETRIES=3
