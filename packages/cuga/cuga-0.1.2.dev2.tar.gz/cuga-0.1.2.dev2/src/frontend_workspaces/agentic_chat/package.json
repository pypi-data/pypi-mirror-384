    "idb": "^8.0.0",
    "immer": "^10.1.1",
    "inversify": "^6.0.2",
    "jose": "^5.9.2",
    "js-yaml": "^4.1.0",
    "jsoneditor": "^10.0.0",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "marked": "^16.3.0",
