{
  "name": "agentic_chat",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "./src/App.tsx",
  "module": "./src/App.tsx",
  "exports": {
    ".": {
      "import": "./src/App.tsx"
    }
  },
  "scripts": {
    "dev": "vite",
    "build-shared": "npm run build -w @uiagent/shared",
    "build": "tsc -b && vite build",
    "build:test": "vite build --mode test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss,md}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,scss,md}'",
    "preview": "vite preview",
    "test": "vitest",
    "test:snapshot": "vite build --mode test && vitest run --testNamePattern=snapshot"
  },
  "devDependencies": {
    "@carbon/ai-chat": "^0.5.1",
    "@carbon/ibm-products": "^2.66.0",
    "@carbon/icons-react": "^11.45.0",
