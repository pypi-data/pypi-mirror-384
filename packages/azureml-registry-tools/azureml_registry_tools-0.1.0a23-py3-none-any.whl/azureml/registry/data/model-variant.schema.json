                    }
                },
                "device": {
                    "type": "string",
                    "enum": ["", "npu", "cpu", "gpu"]
                },
                "executionProvider": {
                    "type": "string"
                },
                "fileSizeBytes": {
                    "type": "integer"
                },
                "vRamFootprintBytes": {
                    "type": "integer"
                }
            },
            "required": [
