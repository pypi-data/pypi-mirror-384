                    "assetId": {
                        "type": "string",
                        "pattern": "^azureml://registries/"
                    }
                },
                "required": [
                    "assetId"
                ]
            }
        },
        "variantMetadata": {
            "type": "object",
            "properties": {
                "modelType": {
                    "type": "string",
