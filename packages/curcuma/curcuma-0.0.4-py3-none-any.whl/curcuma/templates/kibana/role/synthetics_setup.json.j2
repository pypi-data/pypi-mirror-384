{
    "description": "Synthetics usage",
    "kibana": [
        {
            "base": [],
            "feature": {
                "uptime": ["all"],
            },
            "spaces": [
                "{{ space_id }}"
            ]
        }
    ],
    "elasticsearch": {
        "cluster": [
            "manage_ilm",
            "manage_own_api_key",
            "monitor",
            "read_pipeline"
        ],
        "indices": [
            {
                "names": [
                    ".alerts-observability.uptime.alerts-*"
                ],
                "privileges": [
                    "read"
                ]
            },
            {
                "names": [
                    "syntetics-*"
                ],
                "privileges": [
                    "auto_configure",
                    "create_doc",
                    "monitor",
                    "read",
                    "view_index_metadata"
                ]
            }
        ],
        "run_as": []
    },
    "metadata": {}
}
