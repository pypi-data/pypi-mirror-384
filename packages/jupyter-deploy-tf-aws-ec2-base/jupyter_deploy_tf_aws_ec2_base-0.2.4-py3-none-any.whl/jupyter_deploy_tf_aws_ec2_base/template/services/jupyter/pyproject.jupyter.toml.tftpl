[project]
name = "jupyter"
version = "0.2.4"
description = "A basic image with jupyterlab and server-documents"
requires-python = ">=3.12"
dependencies = [
    "jupyter-server-documents>=0.1.0a0",
    "jupyterlab>=4.4.2",
%{ if has_gpu || has_neuron ~}
    "numpy>=2.3.0",
    "torch>=2.8.0",
%{ endif ~}
]