let mary_function a b =
  if a > 0 then
    a * 100
  else if b < 0 then
    b * (-20)
  else
    101
let denis_function a =
  if a < (-100) then
    if a > (-25) then (-123)
    else 0
  else if a < 10 then
    a + 200
  else
    350
