#. module: web_form_banner
#: model_terms:ir.ui.view,arch_db:web_form_banner.view_web_form_banner_rule_form
msgid ""
"<code style=\"white-space: pre-wrap\">\n"
"domain = [(\"partner_id\", \"=\", draft.partner_id.id)]\n"
"if record_id:\n"
"  domain += [(\"id\", \"&amp;lt;\", record_id)]\n"
"last = model.search(domain, order=\"date_order desc, id desc\", limit=1)\n"
"if last:\n"
"  html = \"&amp;lt;strong&amp;gt;Previous order:&amp;lt;/strong&amp;gt; &amp;lt;a href='%s'&amp;gt;%s&amp;lt;/a&amp;gt;\" % (url_for(last), last.name)\n"
"  result = {\"visible\": True, \"html\": html}\n"
"else:\n"
"  result = {\"visible\": False}\n"
"</code>"
msgstr ""
"<code style=\"white-space: pre-wrap\">\n"
"domain = [(\"partner_id\", \"=\", draft.partner_id.id)]\n"
"if record_id:\n"
"  domain += [(\"id\", \"&amp;lt;\", record_id)]\n"
"last = model.search(domain, order=\"date_order desc, id desc\", limit=1)\n"
"if last:\n"
"  html = \"&amp;lt;strong&amp;gt;Ordine precedente:&amp;lt;/strong&amp;gt; "
"&amp;lt;a href='%s'&amp;gt;%s&amp;lt;/a&amp;gt;\" % (url_for(last), "
"last.name)\n"
"  result = {\"visible\": True, \"html\": html}\n"
"else:\n"
"  result = {\"visible\": False}\n"
"</code>"

#. module: web_form_banner
