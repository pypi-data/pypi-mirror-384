                },
                "Suppliers": {
                    "active": true,
                    "file": "join_suppliers.sql"
                },
                "Ship-To Addresses (Customers)": {
                    "active": true,
                    "file": "join_ship-to_addresses_customers.sql"
                },
                "Bill-To Addresses (Customers)": {
                    "active": true,
                    "file": "join_bill-to_addresses_customers.sql"
                }
            }
        },
        "nonempty": {
            "active": true
        },
