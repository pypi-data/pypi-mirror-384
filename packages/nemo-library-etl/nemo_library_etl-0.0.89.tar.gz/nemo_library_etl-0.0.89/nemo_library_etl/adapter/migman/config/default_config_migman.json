{
    "config_version": "0.0.1",
    "etl_directory": "./etl/migman",
    "local_database": "./etl/migman/migman_etl.duckdb",
    "setup": {
        "project_status_file": null,
        "projects": []
    },
    "extract": {
        "extract_active": true,
        "load_to_nemo": true,
        "nemo_project_prefix": "migman_extract_",
        "inforcom": {
            "active": false,
            "odbc_connstr": "Driver={SQL Server};Server=your_server;Database=your_database;UID=your_username;PWD=your_password;",
            "chunk_size": 10000,
            "timeout": 300,
            "table_prefix": "INFOR."
        }
    },
    "transform": {
        "transform_active": true,
        "load_to_nemo": true,
        "nemo_project_prefix": "migman_transform_",
        "join": {
            "active": true,
            "adapter": "",
            "joins": {
                "Customers": {
                    "active": true,
