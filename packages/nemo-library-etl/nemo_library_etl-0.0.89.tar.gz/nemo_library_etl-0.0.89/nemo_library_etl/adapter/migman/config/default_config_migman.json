                "Prospects": {
                    "active": false,
                    "threshold": 90,
                    "primary_key": "VC_Interessent.Interessent",
                    "fields": [
                        "S_Adresse.Name1",
                        "S_Adresse.Staat",
                        "S_Adresse.Ort",
                        "S_Adresse.Name2",
                        "S_Adresse.Name3",
                        "S_Adresse.PLZ",
                        "S_Adresse.Strasse",
                        "S_Adresse.Hausnummer"
                    ]
                },
                "Parts": {
                    "active": false,
                    "threshold": 90,
                    "primary_key": "S_Artikel.Artikel",
                    "fields": [
                        "S_ArtikelSpr.Bezeichnung1",
                        "S_ArtikelSpr.Bezeichnung2",
                        "S_ArtikelSpr.Bezeichnung3",
                        "S_ArtikelSpr.Bezeichnung4"
                    ]
                }
            }
        }
    },
    "load": {
        "load_active": true,
        "entities": [
            "Customers",
            "Bill-To Addresses (Customers)",
            "Ship-To Addresses (Customers)"
        ]
    }
