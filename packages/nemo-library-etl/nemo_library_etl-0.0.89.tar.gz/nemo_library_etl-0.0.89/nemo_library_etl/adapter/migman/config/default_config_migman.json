            "active": true,
            "duplicates": {
                "Customers": {
                    "active": true,
                    "threshold": 90,
                    "primary_key": "S_Kunde.Kunde",
                    "fields": [
                        "S_Adresse.Name1",
                        "S_Adresse.Staat",
                        "S_Adresse.Ort",
                        "S_Adresse.Name2",
                        "S_Adresse.Name3",
                        "S_Adresse.PLZ",
