                    ]
                },
                "Suppliers": {
                    "active": false,
                    "threshold": 90,
                    "primary_key": "S_Lieferant.Lieferant",
                    "fields": [
                        "S_Adresse.Name1",
                        "S_Adresse.Staat",
                        "S_Adresse.Ort",
                        "S_Adresse.Name2",
                        "S_Adresse.Name3",
                        "S_Adresse.PLZ",
                        "S_Adresse.Strasse",
                        "S_Adresse.Hausnummer"
                    ]
