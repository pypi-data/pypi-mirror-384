# adopt to your needs, except for EXP, SDATE, EDATE, MYPATH, MYFILE, which will be auto-replaced
# add your param_, ncvar_ and myvar_ to your $QLC_HOME/config/qlc.conf
# param_C1_pl=("35.212")
# ncvar_C1_pl=("var35")
# myvar_C1_pl=("NH4_as")
# pressure levels
RETRIEVE,
CLASS      = XCLASS,
TYPE       = fc, 
STREAM     = oper,
EXPVER     = EXP,
PARAM      = 35.212,
LEVTYPE    = pl,
GRID       = 1.0/1.0,
TIME       = 00:00:00,
STEP       = 0/6/12/18,
LEVELIST   = 10/20/30/50/70/100/150/200/250/300/400/500/700/850/925/1000,
DATE       = SDATE/to/EDATE,
target     = "MYPATH/MYFILE_C1_pl.grb"
