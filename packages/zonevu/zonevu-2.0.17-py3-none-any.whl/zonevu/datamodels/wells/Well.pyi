    state: str | None = ...
    county: str | None = ...
    region: str | None = ...
    district: str | None = ...
    block: str | None = ...
    land_grid_location: str | None = ...
    license_number: str | None = ...
    license_issue_date: datetime | None = ...
    operator: str | None = ...
    operator_division: str | None = ...
    operator_property_number: str | None = ...
    interest: float | None = ...
