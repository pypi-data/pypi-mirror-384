from ..DataModel import DataModel as DataModel
from .FaultSegment import FaultSegment as FaultSegment
from dataclasses import dataclass, field

@dataclass
class Fault(DataModel):
    symbol: str | None = ...
    thickness: int | None = ...
    color: str | None = ...
    interpreter: str | None = ...
    segments: list[FaultSegment] = field(default_factory=list[FaultSegment])

@dataclass
class FaultEntry(DataModel): ...
