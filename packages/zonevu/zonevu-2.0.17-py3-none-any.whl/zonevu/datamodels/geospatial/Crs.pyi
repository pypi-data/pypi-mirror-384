from .Enums import DistanceUnitsEnum as DistanceUnitsEnum
from dataclasses import dataclass
from dataclasses_json import DataClassJsonMixin

@dataclass
class CrsSpec(DataClassJsonMixin):
    dataclass_json_config = ...
    epsg_code: int | None = ...
    projection: str | None = ...
    zone: str | None = ...
    units: DistanceUnitsEnum | None = ...
    @classmethod
    def from_epsg_code(cls, code: int, units: DistanceUnitsEnum = ...) -> CrsSpec: ...
    @classmethod
    def from_names(cls, projection: str, zone: str, units: DistanceUnitsEnum = ...) -> CrsSpec: ...
    def to_string(self) -> str: ...

@dataclass
class CrsEntry(DataClassJsonMixin):
    dataclass_json_config = ...
    id: str
    name: str
