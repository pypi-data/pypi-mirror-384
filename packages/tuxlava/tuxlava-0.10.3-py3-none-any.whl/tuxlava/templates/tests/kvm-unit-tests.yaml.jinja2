- test:
    timeout:
      minutes: {{ timeout }}
    definitions:
    - repository: {{ test_definitions }}
{%if device.redirect_to_kmsg %}
      lava-signal: kmsg
{% endif %}
      from: url
      compression: zstd
      path: automated/linux/kvm-unit-tests/kvm-unit-tests.yaml
      name: kvm-unit-tests
      parameters:
        SKIP_INSTALL: True
        SMP: True

