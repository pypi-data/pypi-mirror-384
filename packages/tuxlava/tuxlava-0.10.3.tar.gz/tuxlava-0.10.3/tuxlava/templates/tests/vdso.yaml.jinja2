- test:
    timeout:
      minutes: {{ timeout }}
    definitions:
    - repository: {{ test_definitions }}
{%if device.redirect_to_kmsg %}
      lava-signal: kmsg
{% endif %}
      from: url
      compression: zstd
      path: automated/linux/vdsotest/vdsotest.yaml
      name: vdso
      parameters:
        SKIP_INSTALL: True
        VDSOTESTALL: 'all'
        VDSO_INSTALL_PATH: '/opt/vdsotest'

