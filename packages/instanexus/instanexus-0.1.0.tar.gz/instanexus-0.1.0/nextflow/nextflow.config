params {
    input_csv = "inputs/bsa.csv"
    output_dir = "outputs"
    mode = "reference_free"
    chain = ""
}

process {
    executor = 'local'
    cpus = 4
    memory = '8 GB'
    time = '2h'
}

timeline {
    enabled = true
    file = 'timeline.html'
}

report {
    enabled = true
    file = 'report.html'
}

trace {
    enabled = true
    file = 'trace.txt'
}

docker.enabled = false
conda.enabled = true
conda.file = "../environment.osx-arm64.yaml"
