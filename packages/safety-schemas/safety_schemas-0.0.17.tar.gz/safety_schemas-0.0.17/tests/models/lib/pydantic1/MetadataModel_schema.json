{
    "title": "MetadataModel",
    "description": "Main data about the report, used for traceability purposes.",
    "type": "object",
    "properties": {
        "scan_type": {
            "$ref": "#/definitions/ScanType"
        },
        "stage": {
            "$ref": "#/definitions/Stage"
        },
        "scan_locations": {
            "title": "Scan Locations",
            "type": "array",
            "items": {
                "type": "string",
                "format": "path"
            }
        },
        "authenticated": {
            "title": "Authenticated",
            "type": "boolean"
        },
        "authentication_type": {
            "$ref": "#/definitions/AuthenticationType"
        },
        "telemetry": {
            "$ref": "#/definitions/TelemetryModel"
        },
        "schema_version": {
            "$ref": "#/definitions/ReportSchemaVersion"
        },
        "timestamp": {
            "title": "Timestamp",
            "default": "2022-01-01T00:00:00",
            "type": "string",
            "format": "date-time"
        }
    },
    "required": [
        "scan_type",
        "stage",
        "scan_locations",
        "authenticated",
        "authentication_type",
        "telemetry",
        "schema_version"
    ],
    "definitions": {
        "ScanType": {
            "title": "ScanType",
            "description": "An enumeration.",
            "enum": [
                "scan",
                "system-scan",
                "check"
            ]
        },
        "Stage": {
            "title": "Stage",
            "description": "An enumeration.",
            "enum": [
                "development",
                "cicd",
                "production"
            ],
            "type": "string"
        },
        "AuthenticationType": {
            "title": "AuthenticationType",
            "description": "An enumeration.",
            "enum": [
                "token",
                "api_key",
                "unauthenticated"
            ],
            "type": "string"
        },
        "TelemetryModel": {
            "title": "TelemetryModel",
            "description": "Telemetry object generated per Safety report; this model holds data related to the\nclient application running Safety CLI.",
            "type": "object",
            "properties": {
                "safety_options": {
                    "title": "Safety Options",
                    "type": "object",
                    "additionalProperties": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "integer"
                        }
                    }
                },
                "safety_version": {
                    "title": "Safety Version",
                    "type": "string"
                },
                "safety_source": {
                    "title": "Safety Source",
                    "type": "string"
                },
                "os_type": {
                    "title": "Os Type",
                    "type": "string"
                },
                "os_release": {
                    "title": "Os Release",
                    "type": "string"
                },
                "os_description": {
                    "title": "Os Description",
                    "type": "string"
                },
                "python_version": {
                    "title": "Python Version",
                    "type": "string"
                },
                "safety_command": {
                    "title": "Safety Command",
                    "type": "string"
                }
            },
            "required": [
                "safety_options",
                "safety_version",
                "safety_source"
            ]
        },
        "ReportSchemaVersion": {
            "title": "ReportSchemaVersion",
            "description": "An enumeration.",
            "enum": [
                "3.0"
            ]
        }
    }
}