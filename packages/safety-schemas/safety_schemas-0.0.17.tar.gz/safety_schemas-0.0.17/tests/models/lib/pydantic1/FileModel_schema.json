                    }
                },
                "remediation": {
                    "$ref": "#/definitions/RemediationModel"
                }
            },
            "required": [
                "raw",
                "found"
            ]
        },
        "PythonDependency": {
            "title": "PythonDependency",
            "type": "object",
            "properties": {
                "name": {
                    "title": "Name",
                    "type": "string"
                },
                "version": {
                    "title": "Version",
                    "type": "string"
                },
                "specifications": {
                    "title": "Specifications",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/PythonSpecification"
                    }
                },
                "found": {
                    "title": "Found",
                    "type": "string",
                    "format": "path"
                },
                "absolute_path": {
                    "title": "Absolute Path",
                    "type": "string",
                    "format": "path"
                },
                "insecure_versions": {
                    "title": "Insecure Versions",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "secure_versions": {
                    "title": "Secure Versions",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "latest_version_without_known_vulnerabilities": {
                    "title": "Latest Version Without Known Vulnerabilities",
                    "type": "string"
                },
                "latest_version": {
                    "title": "Latest Version",
                    "type": "string"
                },
                "more_info_url": {
                    "title": "More Info Url",
                    "type": "string"
                }
            },
            "required": [
                "name",
                "version",
                "specifications"
            ]
        },
        "IgnoreCodes": {
            "title": "IgnoreCodes",
            "description": "An enumeration.",
            "enum": [
                "unpinned-specification",
                "environment-dependency",
                "cvss-severity",
                "manual"
            ]
        },
        "IgnoredItemDetail": {
            "title": "IgnoredItemDetail",
            "type": "object",
            "properties": {
                "code": {
                    "default": "manual",
                    "allOf": [
                        {
                            "$ref": "#/definitions/IgnoreCodes"
                        }
                    ]
                },
                "reason": {
                    "title": "Reason",
                    "type": "string"
                },
                "expires": {
                    "title": "Expires",
                    "type": "string",
                    "format": "date"
                },
                "specifications": {
                    "title": "Specifications",
                    "type": "array",
                    "items": {},
                    "uniqueItems": true
                }
            }
        },
        "DependencyResultModel": {
            "title": "DependencyResultModel",
            "type": "object",
            "properties": {
                "dependencies": {
                    "title": "Dependencies",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/PythonDependency"
                    }
                },
                "ignored_vulns": {
                    "title": "Ignored Vulns",
                    "type": "object",
                    "additionalProperties": {
                        "$ref": "#/definitions/IgnoredItemDetail"
                    }
                },
                "ignored_vulns_data": {
                    "title": "Ignored Vulns Data",
                    "type": "object",
                    "additionalProperties": {
                        "$ref": "#/definitions/Vulnerability"
                    }
                },
                "failed": {
                    "title": "Failed",
                    "default": false,
                    "type": "boolean"
                }
            },
            "required": [
                "dependencies"
            ]
