                    "type": "string"
                },
                "all_vulnerable_specs": {
                    "title": "All Vulnerable Specs",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "analyzed_version": {
                    "title": "Analyzed Version",
                    "type": "string"
                },
                "analyzed_requirement": {
                    "title": "Analyzed Requirement",
                    "type": "string"
                },
                "advisory": {
                    "title": "Advisory",
                    "type": "string"
                },
                "is_transitive": {
                    "title": "Is Transitive",
                    "type": "boolean"
                },
                "published_date": {
                    "title": "Published Date",
                    "type": "string"
                },
                "fixed_versions": {
                    "title": "Fixed Versions",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "closest_versions_without_known_vulnerabilities": {
                    "title": "Closest Versions Without Known Vulnerabilities",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "resources": {
                    "title": "Resources",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "CVE": {
                    "title": "Cve"
                },
                "severity": {
                    "title": "Severity"
                },
                "affected_versions": {
                    "title": "Affected Versions",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "more_info_url": {
                    "title": "More Info Url",
                    "type": "string"
                }
            },
            "required": [
                "vulnerability_id",
                "package_name",
                "ignored",
                "vulnerable_spec"
            ]
        },
        "ClosestSecureVersion": {
            "title": "ClosestSecureVersion",
            "type": "object",
            "properties": {
                "upper": {
                    "title": "Upper",
                    "type": "string"
                },
                "lower": {
                    "title": "Lower",
                    "type": "string"
                }
            },
            "required": [
                "upper",
                "lower"
            ]
        },
        "RemediationModel": {
            "title": "RemediationModel",
            "type": "object",
            "properties": {
                "vulnerabilities_found": {
                    "title": "Vulnerabilities Found",
                    "type": "integer"
                },
                "more_info_url": {
                    "title": "More Info Url",
                    "type": "string"
                },
                "recommended": {
                    "title": "Recommended",
                    "type": "string"
                },
                "closest_secure": {
                    "$ref": "#/definitions/ClosestSecureVersion"
                },
                "other_recommended": {
                    "title": "Other Recommended",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "vulnerabilities_found",
                "more_info_url",
                "recommended"
            ]
        },
        "PythonSpecification": {
            "title": "PythonSpecification",
            "type": "object",
            "properties": {
                "raw": {
                    "title": "Raw",
                    "type": "string"
                },
                "found": {
                    "title": "Found",
                    "type": "string",
