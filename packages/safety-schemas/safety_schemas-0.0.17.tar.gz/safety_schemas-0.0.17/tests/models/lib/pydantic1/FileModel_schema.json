{
    "title": "FileModel",
    "type": "object",
    "properties": {
        "location": {
            "title": "Location",
            "type": "string",
            "format": "path"
        },
        "file_type": {
            "$ref": "#/definitions/FileType"
        },
        "results": {
            "$ref": "#/definitions/DependencyResultModel"
        }
    },
    "required": [
        "location",
        "file_type"
    ],
    "definitions": {
        "FileType": {
            "title": "FileType",
            "description": "An enumeration.",
            "enum": [
                "requirements.txt",
                "poetry.lock",
                "Pipfile.lock",
                ".safety-project.ini",
                "pyvenv.cfg",
                "pyproject.toml"
            ]
        },
        "Vulnerability": {
            "title": "Vulnerability",
            "type": "object",
            "properties": {
                "vulnerability_id": {
                    "title": "Vulnerability Id",
                    "type": "string"
                },
                "package_name": {
                    "title": "Package Name",
                    "type": "string"
                },
                "ignored": {
                    "title": "Ignored",
                    "type": "boolean"
                },
                "vulnerable_spec": {
                    "title": "Vulnerable Spec"
                },
                "ignored_reason": {
                    "title": "Ignored Reason",
                    "type": "string"
                },
                "ignored_expires": {
                    "title": "Ignored Expires",
                    "type": "string"
                },
                "ignored_code": {
