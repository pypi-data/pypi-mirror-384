{
  "$defs": {
    "ClosestSecureVersion": {
      "properties": {
        "upper": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "title": "Upper"
        },
        "lower": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "title": "Lower"
        }
      },
      "required": [
        "upper",
        "lower"
      ],
      "title": "ClosestSecureVersion",
      "type": "object"
    },
    "DependencyResultModel": {
      "properties": {
        "dependencies": {
          "items": {
            "$ref": "#/$defs/PythonDependency"
          },
          "title": "Dependencies",
          "type": "array"
        },
        "ignored_vulns": {
          "additionalProperties": {
            "$ref": "#/$defs/IgnoredItemDetail"
          },
          "title": "Ignored Vulns",
          "type": "object"
        },
        "ignored_vulns_data": {
          "additionalProperties": {
            "$ref": "#/$defs/Vulnerability"
          },
          "title": "Ignored Vulns Data",
          "type": "object"
        },
        "failed": {
          "default": false,
          "title": "Failed",
          "type": "boolean"
        }
      },
      "required": [
        "dependencies"
      ],
      "title": "DependencyResultModel",
      "type": "object"
    },
    "FileType": {
      "enum": [
        "requirements.txt",
        "poetry.lock",
        "Pipfile.lock",
        ".safety-project.ini",
        "pyvenv.cfg",
        "pyproject.toml"
      ],
      "title": "FileType",
      "type": "string"
    },
    "IgnoreCodes": {
      "enum": [
        "unpinned-specification",
        "environment-dependency",
        "cvss-severity",
        "manual"
      ],
      "title": "IgnoreCodes",
      "type": "string"
    },
    "IgnoredItemDetail": {
      "properties": {
        "code": {
          "$ref": "#/$defs/IgnoreCodes",
          "default": "manual"
        },
        "reason": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Reason"
        },
        "expires": {
          "anyOf": [
            {
              "format": "date",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Expires"
        },
        "specifications": {
          "anyOf": [
            {
              "items": {},
              "type": "array",
              "uniqueItems": true
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Specifications"
        }
      },
      "title": "IgnoredItemDetail",
      "type": "object"
    },
    "PythonDependency": {
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "version": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "title": "Version"
        },
        "specifications": {
          "items": {
            "$ref": "#/$defs/PythonSpecification"
          },
          "title": "Specifications",
          "type": "array"
        },
        "found": {
          "anyOf": [
            {
              "format": "path",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Found"
        },
        "absolute_path": {
          "anyOf": [
            {
              "format": "path",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Absolute Path"
        },
        "insecure_versions": {
          "items": {
            "type": "string"
          },
          "title": "Insecure Versions",
          "type": "array"
        },
        "secure_versions": {
          "items": {
            "type": "string"
          },
          "title": "Secure Versions",
          "type": "array"
        },
        "latest_version_without_known_vulnerabilities": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Latest Version Without Known Vulnerabilities"
        },
        "latest_version": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Latest Version"
        },
        "more_info_url": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "More Info Url"
        }
      },
      "required": [
        "name",
        "version",
        "specifications"
      ],
      "title": "PythonDependency",
      "type": "object"
    },
    "PythonSpecification": {
      "properties": {
        "raw": {
          "title": "Raw",
          "type": "string"
        },
        "found": {
          "anyOf": [
            {
              "format": "path",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "title": "Found"
        },
        "vulnerabilities": {
          "items": {
            "$ref": "#/$defs/Vulnerability"
          },
          "title": "Vulnerabilities",
          "type": "array"
        },
        "remediation": {
          "anyOf": [
            {
              "$ref": "#/$defs/RemediationModel"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        }
      },
      "required": [
        "raw",
        "found"
      ],
      "title": "PythonSpecification",
      "type": "object"
    },
    "RemediationModel": {
      "properties": {
        "vulnerabilities_found": {
          "title": "Vulnerabilities Found",
          "type": "integer"
        },
        "more_info_url": {
          "title": "More Info Url",
          "type": "string"
        },
        "recommended": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "title": "Recommended"
        },
        "closest_secure": {
          "anyOf": [
            {
              "$ref": "#/$defs/ClosestSecureVersion"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        },
        "other_recommended": {
          "items": {
            "type": "string"
          },
          "title": "Other Recommended",
          "type": "array"
        }
      },
      "required": [
        "vulnerabilities_found",
        "more_info_url",
        "recommended"
      ],
      "title": "RemediationModel",
      "type": "object"
    },
    "Vulnerability": {
      "properties": {
        "vulnerability_id": {
          "title": "Vulnerability Id",
          "type": "string"
        },
        "package_name": {
          "title": "Package Name",
          "type": "string"
        },
        "ignored": {
          "title": "Ignored",
          "type": "boolean"
        },
        "vulnerable_spec": {
          "title": "Vulnerable Spec"
        },
        "ignored_reason": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Ignored Reason"
        },
        "ignored_expires": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Ignored Expires"
        },
        "ignored_code": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Ignored Code"
        },
        "all_vulnerable_specs": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "All Vulnerable Specs"
        },
        "analyzed_version": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Analyzed Version"
        },
        "analyzed_requirement": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Analyzed Requirement"
        },
        "advisory": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Advisory"
        },
        "is_transitive": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Is Transitive"
        },
        "published_date": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Published Date"
        },
        "fixed_versions": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Fixed Versions"
        },
        "closest_versions_without_known_vulnerabilities": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Closest Versions Without Known Vulnerabilities"
        },
        "resources": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Resources"
        },
        "CVE": {
          "anyOf": [
            {},
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Cve"
        },
        "severity": {
          "anyOf": [
            {},
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Severity"
        },
        "affected_versions": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Affected Versions"
        },
        "more_info_url": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "More Info Url"
        }
      },
      "required": [
        "vulnerability_id",
        "package_name",
        "ignored",
        "vulnerable_spec"
      ],
      "title": "Vulnerability",
      "type": "object"
    }
  },
  "properties": {
    "location": {
      "format": "path",
      "title": "Location",
      "type": "string"
    },
    "file_type": {
      "$ref": "#/$defs/FileType"
    },
    "results": {
      "$ref": "#/$defs/DependencyResultModel"
    }
  },
  "required": [
    "location",
    "file_type"
  ],
  "title": "FileModel",
  "type": "object"
}