# Backend de extracción: pdfplumber, pymupdf, pypdf2
EXTRACTOR_BACKEND=pdfplumber

# Límite de páginas a escanear (0 = sin límite)
SCAN_FIRST_N=0

# Mínimo de líneas de nota para considerar una página como índice
MIN_NOTE_LINES=3

# Idioma: es, en
LANGUAGE=es

# Nivel de logging: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO
