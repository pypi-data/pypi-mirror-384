<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="helm_chart_secret_form_view" model="ir.ui.view">
        <field name="name">helm.helm_chart_secret_form_view</field>
        <field name="model">helm.chart.secret</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="data_ids" nolabel="1">
                        <list editable="bottom">
                            <field name="key" />
                            <field name="value" widget="password" />
                        </list>
                    </field>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
