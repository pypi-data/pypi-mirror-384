{
    "mode": "dev",
    "memory": {
        "vector_dim": 768,
        "edge_types": ["causes", "similar", "opposite", "part_of", "contains", "implies"]
    },
    "igvam": {
        "goals": {
            "primary": "Assist developers in testing",
            "secondary": "Provide clear debugging information",
            "tertiary": "Support rapid iteration"
        },
        "values": {
            "honesty": 0.95,
            "helpfulness": 0.85,
            "harm_prevention": 1.0,
            "fairness": 0.8,
            "transparency": 0.9,
            "efficiency": 0.7
        }
    },
    "agents": {
        "specs": [
            {
                "id": "reasoning_agent",
                "type": "cognitive",
                "capabilities": ["reasoning", "planning"],
                "replicas": 1
            },
            {
                "id": "memory_agent",
                "type": "retrieval",
                "capabilities": ["search", "indexing"],
                "replicas": 1
            },
            {
                "id": "creative_agent",
                "type": "generative",
                "capabilities": ["ideation", "synthesis"],
                "replicas": 1
            },
            {
                "id": "critique_agent",
                "type": "evaluative",
                "capabilities": ["verification", "critique"],
                "replicas": 1
            },
            {
                "id": "integration_agent",
                "type": "coordinator",
                "capabilities": ["orchestration", "integration"],
                "replicas": 1
            }
        ],
        "communication": "message_passing"
    },
    "environment": {
        "type": "development",
        "config": {
            "interfaces": ["api", "shell", "ui"],
            "observation_space": "multimodal",
            "action_space": "structured"
        }
    },
    "logging": {
        "level": "DEBUG",
        "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s",
        "file": "logs/mrce_dev.log",
        "distributed": false,
        "structured": true
    },
    "use_distributed": false,
    "use_persistence": true,
    "use_llm": true,
    "use_hybrid_reasoning": true,
    "use_evolutionary": true,
    "llm": {
        "provider": "openai",
        "model": "gpt-3.5-turbo",
        "embedding_model": "text-embedding-ada-002",
        "fallbacks": [
            {
                "provider": "local",
                "model": "llama-7b"
            }
        ]
    },
    "database": {
        "type": "sqlite",
        "connection": "mrce_dev.db",
        "replication": false
    },
    "distributed": {
        "ray_address": "local",
        "cluster_resources": {
            "cpu": 4,
            "memory": 8000000000,
            "gpu": 0
        }
    },
    "security": {
        "constraints": {
            "safety": {
                "self_modification_limit": 0.5,
                "igvam_compliance_minimum": 0.7,
                "core_component_protection": ["IGVAM", "AuditTrail"]
            },
            "operational": {
                "max_resource_usage": 0.9,
                "required_redundancy": 1,
                "recovery_capability": true,
                "failover_timeout": 10,
                "rate_limiting": {
                    "enabled": true,
                    "max_requests_per_minute": 1000
                }
            },
            "cryptographic": {
                "audit_signing": false,
                "state_verification": true,
                "integrity_checking": true
            }
        },
        "audit_path": "logs/audit/audit_trail_dev.json"
    },
    "performance": {
        "max_concurrent_ops": 50,
        "min_workers": 1,
        "max_workers": 4,
        "throttling": {
            "enabled": false,
            "thresholds": {
                "cpu": {"warning": 0.8, "critical": 0.95},
                "memory": {"warning": 0.8, "critical": 0.95},
                "io": {"warning": 0.8, "critical": 0.95}
            }
        },
        "caching": {
            "enabled": true,
            "max_size": 1000,
            "ttl": 600
        }
    },
    "deployment": {
        "kubernetes": {
            "namespace": "mrce-dev",
            "resources": {
                "requests": {
                    "memory": "1Gi",
                    "cpu": "0.5"
                },
                "limits": {
                    "memory": "2Gi",
                    "cpu": "1"
                }
            },
            "replicas": 1,
            "deployment_strategy": "rolling"
        },
        "container": {
            "registry": "registry.example.com/mrce",
            "base_image": "python:3.11-slim"
        }
    },
    "monitoring": {
        "metrics": {
            "collection_interval": 5,
            "exporters": ["json", "console"]
        },
        "alerts": {
            "channels": ["console"],
            "contacts": {
                "developer": "dev@example.com"
            }
        },
        "tracing": {
            "enabled": true,
            "sampling_rate": 1.0,
            "exporters": ["console"]
        },
        "health_checks": [
            {
                "name": "memory_service",
                "endpoint": "/health/memory",
                "interval": 15,
                "timeout": 5,
                "healthy_threshold": 1,
                "unhealthy_threshold": 2
            },
            {
                "name": "reasoning_service",
                "endpoint": "/health/reasoning",
                "interval": 15,
                "timeout": 5,
                "healthy_threshold": 1,
                "unhealthy_threshold": 2
            }
        ]
    },
    "debug": {
        "enabled": true,
        "breakpoints": [],
        "hot_reload": true,
        "profiling": {
            "enabled": true,
            "interval": 300
        },
        "verbose_output": true,
        "memory_analysis": true,
        "save_intermediate_states": true
    }
}