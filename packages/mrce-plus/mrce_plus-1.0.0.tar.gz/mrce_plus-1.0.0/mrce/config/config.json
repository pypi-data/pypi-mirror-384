{
    "mode": "dev",
    "memory": {
        "vector_dim": 1024,
        "edge_types": ["causes", "similar", "opposite", "part_of", "contains", "implies"]
    },
    "igvam": {
        "goals": {
            "primary": "Assist users with accurate information",
            "secondary": "Maintain system integrity"
        },
        "values": {
            "honesty": 0.9,
            "helpfulness": 0.8,
            "harm_prevention": 1.0,
            "fairness": 0.7
        }
    },
    "agents": {
        "specs": [
            {
                "id": "reasoning_agent",
                "type": "cognitive",
                "capabilities": ["reasoning", "planning"]
            },
            {
                "id": "memory_agent",
                "type": "retrieval",
                "capabilities": ["search", "indexing"]
            },
            {
                "id": "creative_agent",
                "type": "generative",
                "capabilities": ["ideation", "synthesis"]
            }
        ],
        "communication": "shared_memory"
    },
    "environment": {
        "type": "simulation",
        "config": {
            "max_steps": 1000,
            "observation_space": "text",
            "action_space": "structured"
        }
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s",
        "file": "mrce.log"
    },
    "use_distributed": false,
    "use_persistence": false,
    "use_llm": true,
    "use_hybrid_reasoning": true,
    "use_evolutionary": false,
    "llm": {
        "provider": "openai",
        "model": "gpt-4",
        "embedding_model": "text-embedding-ada-002"
    },
    "database": {
        "type": "sqlite",
        "connection": "memory.db"
    },
    "security": {
        "constraints": {
            "safety": {
                "self_modification_limit": 0.3,
                "igvam_compliance_minimum": 0.8,
                "core_component_protection": ["CausalIntegrityWatchdog", "IGVAM", "AuditTrail"]
            },
            "operational": {
                "max_resource_usage": 0.9,
                "required_redundancy": 2,
                "recovery_capability": true
            }
        },
        "audit_path": "audit_trail.json"
    },
    "performance": {
        "max_concurrent_ops": 100,
        "min_workers": 2,
        "max_workers": 8
    }
}