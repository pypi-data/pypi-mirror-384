_ort-build-tab-complete() {
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD - 1]}"
    opts="build update"

    if [[ ${cur} == * ]]; then
        COMPREPLY=($(compgen -W "${opts}" -- ${cur}))
        return 0
    fi
}
complete -F _ort-build-tab-complete ort-build
complete -F _ort-build-tab-complete ortbuild
