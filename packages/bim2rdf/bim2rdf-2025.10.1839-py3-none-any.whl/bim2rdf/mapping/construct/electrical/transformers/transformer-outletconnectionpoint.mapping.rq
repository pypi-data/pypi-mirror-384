PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX s223: <http://data.ashrae.org/standard223#>
PREFIX spkl: <http://speckle.systems/> 
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX meta: <http://meta>
PREFIX qudt: <http://qudt.org/schema/qudt/>
PREFIX quantitykind: <http://qudt.org/vocab/quantitykind/> 
PREFIX unit: <http://qudt.org/vocab/unit/>

CONSTRUCT {
?tr s223:cnx ?conn.
?conn  a s223:OutletConnectionPoint.
?conn rdfs:label "OutletConnectionPoint"^^xsd:string.
?conn s223:hasMedium s223:Electricity-AC. 

}
WHERE 
{
?tr a s223:ElectricEnergyTransformer.
<<?tr spkl:category  "Electrical Equipment">> meta: <<?mi spkl:name "electrical/transformers">>.
?tr spkl:Connectors ?l.
?l  ?in ?r.# list item
?r  spkl:referencedId ?conn.
}