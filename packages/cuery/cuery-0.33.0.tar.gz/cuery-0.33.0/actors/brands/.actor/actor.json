{
    "actorSpecification": 1,
    "name": "brand-researcher",
    "title": "Brand Researcher",
    "description": "Discover competitors and fetch detailed brand information using LLM-powered search.",
    "version": "0.1",
    "buildTag": "latest",
    "dockerfile": "./Dockerfile",
    "dockerContextDir": "../..",
    "input": "./input_schema.json",
    "storages": {
        "dataset": "./dataset_schema.json"
    },
    "main": "python -m cuery.actors.brands",
    "readme": "./README.md",
    "minMemoryMbytes": 256,
    "maxMemoryMbytes": 4096,
    "usesStandbyMode": false,
    "environmentVariables": {
        "OPENAI_API_KEY": "@OPENAI_API_KEY"
    }
}