{
    "actorSpecification": 1,
    "name": "funnel-keywords",
    "title": "Funnel Keyword Generator",
    "description": "Generate keyword ideas across marketing funnel stages using LLM-powered seed generation and Google Keyword Planner.",
    "version": "0.1",
    "buildTag": "latest",
    "dockerfile": "./Dockerfile",
    "dockerContextDir": "../..",
    "input": "./input_schema.json",
    "storages": {
        "dataset": "./dataset_schema.json"
    },
    "main": "python -m cuery.actors.funnels",
    "readme": "./README.md",
    "minMemoryMbytes": 256,
    "maxMemoryMbytes": 4096,
    "usesStandbyMode": false,
    "environmentVariables": {
        "OPENAI_API_KEY": "@OPENAI_API_KEY",
        "GOOGLE_ADS_DEVELOPER_TOKEN": "@GOOGLE_ADS_DEVELOPER_TOKEN",
        "GOOGLE_ADS_LOGIN_CUSTOMER_ID": "@GOOGLE_ADS_LOGIN_CUSTOMER_ID",
        "GOOGLE_ADS_USE_PROTO_PLUS": "true",
        "GOOGLE_ADS_JSON_KEY": "@GOOGLE_ADS_JSON_KEY"
    }
}