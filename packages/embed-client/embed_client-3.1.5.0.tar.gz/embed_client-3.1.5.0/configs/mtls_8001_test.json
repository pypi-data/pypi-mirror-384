{
  "server": {
    "host": "https://localhost",
    "port": 8001
  },
  "auth": {
    "method": "certificate",
    "certificate": {
      "cert_file": "certs/client.crt",
      "key_file": "certs/client.key"
    }
  },
  "ssl": {
    "enabled": true,
    "verify_mode": "CERT_NONE",
    "check_hostname": false,
    "check_expiry": false,
    "ca_cert_file": "certs/ca.crt",
    "cert_file": "certs/client.crt",
    "key_file": "certs/client.key"
  }
}
