{
  "server": {
    "host": "localhost",
    "port": 10003
  },
  "auth": {
    "method": "api_key"
  },
  "ssl": {
    "enabled": false
  },
  "security": {
    "enabled": true,
    "tokens": {
      "admin": "admin-secret-key",
      "user": "user-secret-key",
      "readonly": "readonly-secret-key"
    },
    "roles": {
      "admin": [
        "read",
        "write",
        "delete",
        "admin"
      ],
      "user": [
        "read",
        "write"
      ],
      "readonly": [
        "read"
      ]
    }
  }
}