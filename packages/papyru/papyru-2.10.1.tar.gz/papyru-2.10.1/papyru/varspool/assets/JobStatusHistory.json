{
  "required": [
    "location",
    "items"
  ],
  "properties": {
    "location": {
      "type": "string",
      "example": "https://varspooltest.puzzleyou.net/queues/example/jobs/1/status"
    },
    "items": {
      "type": "array",
      "items": {
        "required": [
          "status",
          "checksum",
          "date",
          "data"
        ],
        "properties": {
          "status": {
            "type": "string",
            "example": "open"
          },
          "checksum": {
            "type": "string",
            "example": "936DA01F9ABD4d9d80C702AF85C822A8"
          },
          "date": {
            "type": "string",
            "format": "date-time",
            "example": "1970-01-01T00:00:00+00:00"
          },
          "data": {
            "type": "object",
            "description": "contains custom status information e.g. for the worker",
            "example": {
              "url": "https://example.org/some_image.svg",
              "data_sink": "https://printdata.imagestore/images",
              "filters": {
                "stretch": {
                  "width": 1920,
                  "height": 1060
                },
                "rotate": "landscape",
                "convert": "image/jpeg"
              }
            }
          }
        }
      }
    }
  }
}
