      "targets": [
        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${sourceTEM}"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH all_durations AS (\n  SELECT\n    time::date AS day,\n    'epu' AS source,\n    EXTRACT(EPOCH FROM running_duration) AS running_seconds\n  FROM epu_running_daily\n  WHERE $__timeFilter(time)\n    AND instrument_id = $instrument\n\n  UNION ALL\n\n  SELECT\n    time::date AS day,\n    'tomo' AS source,\n    EXTRACT(EPOCH FROM running_duration) AS running_seconds\n  FROM tomo_running_daily\n  WHERE $__timeFilter(time)\n    AND instrument_id = $instrument\n),\n\ndays AS (\n  SELECT generate_series(\n    $__timeFrom()::date,\n    $__timeTo()::date,\n    interval '1 day'\n  ) AS day\n)\n\nSELECT\n  d.day AS time,\n  ROUND(100.0 * COALESCE(SUM(running_seconds) FILTER (WHERE source = 'epu'), 0) / 86400.0, 2) AS \"EPU\",\n  ROUND(100.0 * COALESCE(SUM(running_seconds) FILTER (WHERE source = 'tomo'), 0) / 86400.0, 2) AS \"Tomo\"\nFROM days d\nLEFT JOIN all_durations ad ON ad.day = d.day\nGROUP BY d.day\nORDER BY d.day;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Utilization",
      "type": "barchart"
    },
