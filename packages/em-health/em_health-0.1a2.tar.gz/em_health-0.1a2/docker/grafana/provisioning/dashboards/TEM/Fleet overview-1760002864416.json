          "id": "organize",
          "options": {
            "excludeByName": {},
            "includeByName": {},
            "indexByName": {},
            "renameByName": {
              "epu_utilization_percent": "EPU",
              "tomo_utilization_percent": "Tomo"
            }
          }
        }
      ],
      "type": "gauge"
