          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "-- Total EPU time\nWITH epu_sum AS (\n  SELECT\n    EXTRACT(EPOCH FROM SUM(running_duration)) AS total_epu_seconds\n  FROM epu_running_daily\n  WHERE instrument_id = $instrument\n    AND $__timeFilter(time)\n),\n\n-- Total Tomo time\ntomo_sum AS (\n  SELECT\n    EXTRACT(EPOCH FROM SUM(running_duration)) AS total_tomo_seconds\n  FROM tomo_running_daily\n  WHERE instrument_id = $instrument\n    AND $__timeFilter(time)\n)\n\n-- Final result\nSELECT\n  ROUND(COALESCE(e.total_epu_seconds, 0) / NULLIF(($__to - $__from) / 1000.0, 0) * 100.0, 2) AS epu_utilization_percent,\n  ROUND(COALESCE(t.total_tomo_seconds, 0) / NULLIF(($__to - $__from) / 1000.0, 0) * 100.0, 2) AS tomo_utilization_percent\nFROM epu_sum e\nCROSS JOIN tomo_sum t;\n",
          "refId": "uptime",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
